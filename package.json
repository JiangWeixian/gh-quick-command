{
  "name": "gh-quick-command",
  "version": "0.3.0-2",
  "description": "quick open github page with command",
  "main": ".lib/ghp.cjs.js",
  "module": ".lib/ghp.esm.js",
  "bin": {
    "ghp": "lib/ghp",
    "ghp-my": "lib/ghp-my",
    "ghp-new": "lib/ghp-new",
    "ghp-open": "lib/ghp-open"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf ./lib && rimraf ./.lib && tsc",
    "build": "npm run prebuild && node scripts/build.js",
    "pretest": "npm run build",
    "update": "npm update && npm update --save-dev",
    "check": "npm outdated && npm outdated --save-dev",
    "prepublishOnly": "npm run build && np --no-cleanup --yolo --no-publish --any-branch"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "github",
    "command",
    "zsh"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JiangWeixian/gh-quick-command"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "git-repo-info": "^2.1.1",
    "git-repo-name": "^1.0.1",
    "git-user-name": "2.0.0"
  },
  "devDependencies": {
    "@types/git-repo-name": "^1.0.0",
    "@types/git-user-name": "2.0.0",
    "@types/node": "12.6.8",
    "@types/rimraf": "2.0.2",
    "builtin-modules": "3.1.0",
    "fs-extra": "9.0.0",
    "husky": "3.0.2",
    "np": "5.0.3",
    "prettier": "1.17.0",
    "pretty-quick": "1.10.0",
    "rimraf": "2.6.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
